<!DOCTYPE html>
<html>
  <head>
    <title>Kampus Ruby on rails</title>
    <%= csrf_meta_tags %>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
      <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
	<%= render "material/header" %>
 	  <main>
		<div class="container">
			<%= render "material/message" %>
			<%= yield %>
		</div>
	  </main>
		<%= render "material/footer" %>
    <script>
        $('.datepicker').pickadate({
          selectMonths: true,
          selectYears: 90,
          max: -90,
          });
        $(".button-collapse").sideNav();
    </script>
  </body>
</html>
